{
    "num_workers": 2,
    "webrtc_enabled": true,
    "webrtc_port": 8080,
    "models": {
        "detection": {
            "type": "onnx",
            "path": "models/yolov5s.onnx",
            "input_size": [640, 640],
            "confidence_threshold": 0.5,
            "nms_threshold": 0.4
        },
        "segmentation": {
            "type": "onnx",
            "path": "models/yolov5s-seg.onnx",
            "input_size": [640, 640],
            "confidence_threshold": 0.5,
            "nms_threshold": 0.4
        }
    },
    "communication": {
        "input_port": 8081,
        "output_port": 8082,
        "websocket_enabled": true,
        "signaling_port": 8083
    },
    "webrtc": {
        "stun_servers": [
            "stun:stun.l.google.com:19302",
            "stun:stun1.l.google.com:19302"
        ],
        "turn_servers": [],
        "video_codec": "VP8",
        "audio_enabled": false,
        "bitrate_kbps": 2000
    },
    "rtsp": {
        "enabled": true,
        "sources": [
            {
                "id": "camera_01",
                "url": "rtsp://localhost:8554/camera_01",
                "analysis_type": "object_detection",
                "retry_interval_ms": 5000,
                "max_retries": 10
            }
        ],
        "buffer_size": 1,
        "timeout_ms": 30000
    },
    "performance": {
        "max_queue_size": 100,
        "timeout_ms": 5000,
        "target_fps": 30
    }
}