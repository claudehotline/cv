{
    "num_workers": 2,
    "webrtc_enabled": true,
    "webrtc_port": 8080,
    "models": {
        "detection": {
            "type": "onnx",
            "path": "model/yolov12x.onnx",
            "input_size": [
                640,
                640
            ],
            "confidence_threshold": 0.65,
            "nms_threshold": 0.45
        },
        "segmentation": {
            "type": "onnx",
            "path": "model/yolov12x.onnx",
            "input_size": [
                640,
                640
            ],
            "confidence_threshold": 0.65,
            "nms_threshold": 0.45
        }
    },
    "communication": {
        "input_port": 8081,
        "output_port": 8082,
        "websocket_enabled": true,
        "signaling_port": 8083
    },
    "video_encoding": {
        "mode": "jpeg",
        "jpeg": {
            "quality": 75,
            "max_width": 640,
            "max_height": 480
        },
        "h264": {
            "width": 1280,
            "height": 720,
            "fps": 30,
            "bitrate": 1500000,
            "gop_size": 30,
            "preset": "fast",
            "profile": "baseline",
            "use_hardware": true
        }
    },
    "webrtc": {
        "stun_servers": [
            "stun:stun.l.google.com:19302",
            "stun:stun1.l.google.com:19302"
        ],
        "turn_servers": [],
        "video_codec": "VP8",
        "audio_enabled": false,
        "bitrate_kbps": 2000
    },
    "rtsp": {
        "enabled": true,
        "sources": [
            {
                "id": "camera_01",
                "url": "rtsp://localhost:8554/camera_01",
                "analysis_type": "object_detection",
                "retry_interval_ms": 5000,
                "max_retries": 10
            }
        ],
        "buffer_size": 1,
        "timeout_ms": 30000
    },
    "performance": {
        "max_queue_size": 5,
        "timeout_ms": 5000,
        "target_fps": 30
    },
    "inference": {
        "device": "auto",
        "cuda": {
            "device_id": 0,
            "gpu_mem_limit": 2147483648,
            "cudnn_conv_algo_search": "DEFAULT"
        },
        "cpu": {
            "num_threads": 4,
            "use_arena": true
        },
        "enable_profiling": false
    }
}